<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    <path android:pathData="M0,0h108v108h-108z">
        <aapt:attr name="android:fillColor">
            <!-- 4 colors linear top to bottom-->
            <gradient
                android:endX="54"
                android:endY="108"
                android:startX="54"
                android:startY="0"
                android:type="linear">
                <!-- green200 from top to green500 54% stop -->
                <item
                    android:color="@color/green200"
                    android:offset="0.0" />
                <item
                    android:color="@color/green500"
                    android:offset="0.54" />
                <!-- green600 to 72% stop -->
                <item
                    android:color="@color/green600"
                    android:offset="0.72" />
                <!-- green800 to bottom -->
                <item
                    android:color="@color/green800"
                    android:offset="1.0" />
            </gradient>

        </aapt:attr>
    </path>
</vector>
